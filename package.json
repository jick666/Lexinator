{
  "name": "Lexinator",
  "version": "1.0.0",
  "description": "A modular, adaptive, experimental JavaScript lexer.",
  "main": "src/index.js",
  "type": "module",
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.cjs --coverage",
    "build": "echo \"Nothing to build\"",
    "prepare": "husky",
    "gentree": "node ./.github/workflows/scripts/genTree.js",
    "tree": "yarn gentree",
    "diag": "node ./src/utils/diagnostics.js",
    "diagnostics": "yarn diag",
    "workflow": "yarn lint && yarn test --coverage && node tests/benchmarks/lexer.bench.js"
  },
  "keywords": [
    "lexer",
    "javascript",
    "codex",
    "automation"
  ],
  "license": "MIT",
  "dependencies": {
    "unicode-match-property-ecmascript": "^2.0.0",
    "unicode-match-property-value-ecmascript": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jest/globals": "^29.0.0",
    "@octokit/rest": "^22.0.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "nock": "^13.3.0"
  },
  "engines": {
    "node": ">=18"
  }
}
